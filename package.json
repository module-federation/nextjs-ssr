{
  "public": true,
  "name": "@module-federation/nextjs-ssr",
  "version": "0.0.4-beta.1",
  "description": "Module Federation CSR & SSR Support for Next.js",
  "main": "load.js",
  "types": "index.d.ts",
  "repository": "https://github.com/module-federation/nextjs-ssr",
  "author": "Zack Jackson <zackary.l.jackson@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prettier": "prettier --write \"**/*.{js,json,md,ts,tsx}\"",
    "build": "rollup --config rollup.config.js && rollup --config rollup.flushchunks.config.js && bytenode --compile bundle.js && mv bundle.jsc compiled.jsc",
    "postinstall": "bytenode --compile bundle.js && mv bundle.jsc compiled.jsc && rm bundle.js",
    "prepublishOnly": "yarn build"
  },
  "publishConfig": {
    "registry": "https://r.privjs.com"
  },
  "dependencies": {
    "bytenode": "^1.3.4",
    "chalk": "4.1.2",
    "lodash.clonedeep": "^4.5.0",
    "webpack-federated-stats-plugin": "^2.0.9"
  },
  "peerDependencies": {
    "next": "^12.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "node-fetch": "^3.1.1",
    "prettier": "2.3.2",
    "rollup": "^2.64.0",
    "rollup-obfuscator": "^2.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "webpack": "^5.65.0"
  }
}
